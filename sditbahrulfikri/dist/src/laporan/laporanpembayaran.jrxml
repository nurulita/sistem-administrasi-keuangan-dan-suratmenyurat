	<queryString>
		<![CDATA[SELECT
     pembayaran.`total` AS pembayaran_total,
     pembayaran.`tanggal` AS pembayaran_tanggal,
     pembayaran.`id_catat` AS pembayaran_id_catat,
     pembayaran.`nis` AS pembayaran_nis,
     pembayaran.`namasiswa` AS pembayaran_namasiswa,
     pembayaran.`kelas` AS pembayaran_kelas,
     pembayaran.`namabayar` AS pembayaran_namabayar,
     pembayaran.`carapembayaran` AS pembayaran_carapembayaran
FROM
     `pembayaran` pembayaran
WHERE
     pembayaran.`tanggal`BETWEEN $P{tgl1} AND $P{tgl2}]]>
	</queryString>
